        <mxCell id="IJmZggZovkUzVdCnAwnD-50" value="&lt;b&gt;Episode Start&lt;/b&gt;&lt;br&gt;t = 0" style="ellipse;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=13;" vertex="1" parent="1">
