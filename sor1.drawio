        <mxCell id="IJmZggZovkUzVdCnAwnD-54" value="&lt;b&gt;Compute Reward&lt;/b&gt;&lt;br&gt;&lt;br&gt;r&lt;sub&gt;t&lt;/sub&gt; = -n&lt;sub&gt;iter&lt;/sub&gt;&lt;br&gt;&lt;br&gt;r&lt;sub&gt;t+1&lt;/sub&gt; = ||Ax&lt;sub&gt;t+1&lt;/sub&gt; - b||" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#fff2cc;strokeColor=#d6b656;fontSize=13;" vertex="1" parent="1">
