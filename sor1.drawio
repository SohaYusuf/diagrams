        <mxCell id="IJmZggZovkUzVdCnAwnD-55" value="&lt;b&gt;Terminal?&lt;/b&gt;&lt;br&gt;&lt;br&gt;r&lt;sub&gt;t+1&lt;/sub&gt; ≤ τ&lt;br&gt;&lt;br&gt;(target tolerance)" style="rhombus;whiteSpace=wrap;html=1;fillColor=#ffe6cc;strokeColor=#d79b00;fontSize=13;" vertex="1" parent="1">
