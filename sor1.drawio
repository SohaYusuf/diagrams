        <mxCell id="IJmZggZovkUzVdCnAwnD-58" value="&lt;b&gt;Episode End&lt;/b&gt;&lt;br&gt;&lt;br&gt;Converged!" style="ellipse;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontSize=13;" vertex="1" parent="1">
